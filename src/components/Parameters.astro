

---
var account = Astro.cookies.get("account")?.value ?? ""
const accounturl = Astro.url.searchParams.get("account")
if (accounturl){
  account = accounturl
  Astro.cookies.set("account", accounturl)
}
var org = Astro.cookies.get("org")?.value ?? ""
const orgurl = Astro.url.searchParams.get("org")
if (orgurl){
  org = orgurl
  Astro.cookies.set("org", orgurl)
}
var project = Astro.cookies.get("project")?.value ?? ""
const projecturl = Astro.url.searchParams.get("project")
if (projecturl){
  project = projecturl
  Astro.cookies.set("project", projecturl)
}

